{
  "name": "@ttungbmt/fuse-base",
  "version": "0.0.1",
  "description": "Fuse Base",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "clean": "rimraf node_modules .turbo dist"
  },
  "dependencies": {
    "@tippyjs/react": "^4.2.6",
    "clsx": "^1.2.1",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react-device-detect": "^2.2.2"
  },
  "devDependencies": {
    "@egoist/prettier-config": "^1.0.0",
    "@swc/core": "^1.3.18",
    "@tsconfig/create-react-app": "^1.0.3",
    "@tsconfig/recommended": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3",
    "vitest": "^0.25.2"
  },
  "peerDependencies": {
    "@mui/material": ">=5",
    "@mui/system": ">=5",
    "@mui/utils": ">=5",
    "react": ">=18",
    "react-dom": ">=18",
    "prop-types": ">=15"
  },
  "publishConfig": {
    "access": "public"
  }
}
